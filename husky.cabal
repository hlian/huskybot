name:                husky
version:             1.0.0.0
-- synopsis:            
-- description:         
-- license:             
license-file:        LICENSE
author:              Hao Lian
maintainer:          me@haolian.org
-- copyright:           
-- category:            
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

executable husky
  main-is:             Main.hs
  -- other-modules:       
  -- other-extensions:    
  build-depends:       aeson >= 0.7.0.6,
                       word8 >= 0.1.0,
                       attoparsec >= 0.11.3.4,
                       wreq
                     , mtl
                     , base
                     , text-format
                     , transformers
                     , containers
                     , bytestring
                     , json
                     , utf8-string
                     , text
                     , directory
                     , lens
                     , EitherT >= 0.2.0
                     , random >= 1.0.1.1
                     , array >= 0.4.0.1
                     , warp >=3.0 && <3.1
                     , http-types >0.8 && <0.9
                     , wai >=3.0 && <3.1

  -- hs-source-dirs:      
  default-language:    Haskell2010




